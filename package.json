{
	"name": "essay-grader",
	"version": "1.0.0",
	"private": true,
	"description": "an AI project for scoring essays",
	"main": "src/client/index.js",
	"scripts": {
		"watch": "rm -rf src/server/static/.hot && webpack --watch --config src/config/webpack.config.dev.js",
		"dev": "rm -rf src/server/static/.hot && webpack --config src/config/webpack.config.dev.js",
		"build": "rm -rf src/server/static/.hot && webpack --config src/config/webpack.config.prod.js --progress",
		"serve": "python3 app.py | npm run watch",
		"serve:win": "python app.py | npm run watch",
		"start": "npm run build && python3 app.py",
		"flow": "flow",
		"analyze": "source-map-explorer src/server/static/main.*",
		"flow-type": "flow-typed install",
		"postinstall": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/timdereaper1/AI-project.git"
	},
